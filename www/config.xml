<?xml version='1.0' encoding='utf-8'?>
<widget id="com.jgstudios.boostraplite" version="1.0.0" 
xmlns="http://www.w3.org/ns/widgets" 
xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Bootstrap Assistant Lite</name>
    <description>
        A blank PhoneGap app.
    </description>
    <author email="support@jgstudios.space" 
    href="http://jgstudios.space">
        PhoneGap Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-navigation href="*" />
    <allow-intent href="*" />
    <platform name="android">
        <preference name="android-minSdkVersion" value="17" />
        <allow-intent href="market:*" />
    </platform>
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />
    <preference name="orientation" value="portrait" />
    <preference name="android-build-tool" value="gradle" /> 
    <plugin name="cordova-plugin-admobpro" source="npm" ><variable name="ADMOB_APP_ID" value="ca-app-pub-YOUR_ADMOB_ID" /></plugin>
    <preference name="android-build-tool" value="gradle" />
    <preference name="phonegap-version" value="cli-8.0.0" />
    <icon src="icon144.png" />
</widget>